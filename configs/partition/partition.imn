node n1 {
    type router
    model router
    network-config {
	hostname n1
	!
	interface eth0
	 ip address 10.0.0.1/24
	 ipv6 address 2001:0::1/64
	!
    }
    canvas c1
    iconcoords {232.0 176.0}
    labelcoords {232.0 208.0}
    interface-peer {eth0 n2}
    services {DTLSR OSPFv2 OSPFv3 zebra IPForward}
}

node n2 {
    type router
    model router
    network-config {
	hostname n2
	!
	interface eth1
	 ip address 10.0.1.1/24
	 ipv6 address 2001:1::1/64
	!
	interface eth0
	 ip address 10.0.0.2/24
	 ipv6 address 2001:0::2/64
	!
    }
    canvas c1
    iconcoords {188.0 315.0}
    labelcoords {188.0 347.0}
    interface-peer {eth0 n1}
    interface-peer {eth1 n3}
    services {DTLSR OSPFv2 OSPFv3 zebra IPForward}
}

node n3 {
    type router
    model router
    network-config {
	hostname n3
	!
	interface eth0
	 ip address 10.0.1.2/24
	 ipv6 address 2001:1::2/64
	!
    }
    canvas c1
    iconcoords {293.0 411.0}
    labelcoords {293.0 443.0}
    interface-peer {eth0 n2}
    services {DTLSR OSPFv2 OSPFv3 zebra IPForward}
}

node n4 {
    type router
    model router
    network-config {
	hostname n4
	!
	interface eth0
	 ip address 10.0.2.1/24
	 ipv6 address 2001:2::1/64
	!
    }
    canvas c1
    iconcoords {379.0 186.0}
    labelcoords {379.0 218.0}
    interface-peer {eth0 n5}
    services {DTLSR OSPFv2 OSPFv3 zebra IPForward}
