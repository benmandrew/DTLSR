    network-config {
	hostname n3
	!
	interface eth0
	 ip address 10.0.1.11/24
	 ipv6 address 2001:1::11/64
	!
    }
    canvas c1
    iconcoords {487.0 445.0}
    labelcoords {487.0 477.0}
    services {Routes DefaultRoute SSH}
    interface-peer {eth0 n5}
}

node n4 {
    type router
