node n5 {
    type hub
    network-config {
	hostname n5
	!
    }
    canvas c1
    iconcoords {357.0 383.0}
    labelcoords {357.0 407.0}
    interface-peer {e1 n4}
    interface-peer {e2 n3}
    interface-peer {e3 n6}
}

node n6 {
    type router
    model router
    network-config {
	hostname n6
	!
	interface eth1
	 ip address 10.0.2.1/24
	 ipv6 address 2001:2::1/64
	!
	interface eth0
	 ip address 10.0.1.2/24
	 ipv6 address 2001:1::2/64
	!
    }
    canvas c1
    iconcoords {587.0 314.0}
    labelcoords {587.0 346.0}
    interface-peer {eth0 n5}
    interface-peer {eth1 n2}
}

link l1 {
    nodes {n1 n2}
    bandwidth 0
}

link l3 {
    nodes {n5 n4}
    bandwidth 0
}

link l4 {
    nodes {n5 n3}
    bandwidth 0
}

link l5 {
    nodes {n5 n6}
    bandwidth 0
}

link l6 {
    nodes {n6 n2}
    bandwidth 0
}

canvas c1 {
    name {Canvas1}
}

option global {
    interface_names no
    ip_addresses yes
    ipv6_addresses no
    node_labels yes
    link_labels yes
    show_api no
    background_images no
    annotations yes
    grid yes
    traffic_start 0
}

option session {
}

