node n1 {
    type router
    model host
    network-config {
	hostname n1
	!
	interface eth0
	 ip address 10.0.0.10/24
	 ipv6 address 2001:0::10/64
	!
    }
    canvas c1
    iconcoords {278.0 150.0}
    labelcoords {278.0 182.0}
    interface-peer {eth0 n2}
    services {Routes DefaultRoute SSH}
}

node n2 {
    type router
    model router
    network-config {
	hostname n2
	!
	interface eth2
	 ip address 10.0.2.2/24
	 ipv6 address 2001:2::2/64
	!
	interface eth0
	 ip address 10.0.0.1/24
	 ipv6 address 2001:0::1/64
	!
    }
    canvas c1
    iconcoords {434.0 231.0}
    labelcoords {434.0 263.0}
    interface-peer {eth0 n1}
    services {Routes IPForward}
    interface-peer {eth2 n6}
}

node n3 {
